{
  "name": "playwright-examples",
  "version": "1.0.0",
  "description": "Example running saucectl with playwright.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "postinstall": "npx playwright install",
    "test.reporter": "npx playwright test --project=\"Playwright Reporter\"",
    "test.grid": "npx playwright test --project=grid --reporter=list",
    "test.saucectl": "saucectl run",
    "test.local": "PLAYWRIGHT_PROJECT=local npx playwright test --project=local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saucelabs-training/demo-js.git"
  },
  "keywords": [],
  "author": "",
