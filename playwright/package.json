  "bugs": {
    "url": "https://github.com/saucelabs-training/demo-js/issues"
  },
  "homepage": "https://github.com/saucelabs-training/demo-js#readme",
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "saucectl": "^0.197.0"
  },
  "dependencies": {
    "@saucelabs/playwright-reporter": "^1.5.0"
  }
}
